{
  "manifest_version": 2,
  "name": "vriska",
  "description": "xkit vriska",
  "version": "0.0.1",
  "icons": {
    "179": "icons/vriska-179.png",
    "64": "icons/vriska-64.png",
    "32": "icons/vriska-32.png"
  },
  "permissions": [
      "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.tumblr.com/*"
      ],
      "run_at": "document_idle",
      "js": [
        "vendor/browser-polyfill.min.js",
        "inject.js"
      ]
    }
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "179": "icons/vriska-179.png",
      "64": "icons/vriska-64.png",
      "32": "icons/vriska-32.png"
    },
    "default_title": "XKit",
    "default_popup": "action/menu.html"
  },
  "web_accessible_resources": [
    "src/*.js"
  ]
}
